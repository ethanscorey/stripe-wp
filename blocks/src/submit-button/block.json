{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "stripe-wp/submit-button",
	"version": "0.1.0",
	"title": "Donate Button",
	"category": "stripe-wp",
	"icon": "yes-alt",
	"description": "A customizable button for donation form submissions.",
	"parent": [ "stripe-wp/donate-form" ],
	"example": {},
	"supports": {
		"align": true,
		"color": true,
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		}
	},
	"attributes": {
		"align": {
			"type": "string"
		},
		"buttonText": {
			"type": "string",
			"source": "html",
			"selector": "button"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#000"
		},
		"hoverBackgroundColor": {
			"type": "string",
			"default": "#fff"
		},
		"hoverTextColor": {
			"type": "string",
			"default": "#000"
		},
		"textColor": {
			"type": "string",
			"default": "#fff"
		},
		"fontSize": {
			"type": "string",
			"default": "medium"
		},
		"textAlign": {
			"type": "string",
			"default": "center"
		}
	},
	"textdomain": "stripe-wp",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
